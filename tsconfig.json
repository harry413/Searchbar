{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "Components/search/SearchContainer.jsx", "Components/search/SearchFilters.jsx", "Components/search/SearchInput.jsx", "Components/search/SearchResultCard.jsx", "Components/search/SearchResultItem.jsx", "Components/search/SearchResults.jsx", "Pages/Search.jsx"],
  "exclude": ["node_modules"]
}
